    function _doPostBack(eventTarget, eventArgument) {
        theForm.__EVENTTARGET.value = eventTarget;
        theForm.__EVENTARGUMENT.value = eventArgument;
        theForm.submit();
    }

    function _moveItemFromAtoB_SPLookup(ListBoxA, ListBoxB) {
        var x = document.getElementById(ListBoxA);
        var y = document.getElementById(ListBoxB);
        for (i = 0; i < x.options.length; i++) {
            if (x.options[i].selected) {
                var item = document.createElement("option");
                item.text = x.options[i].text;
                item.value = x.options[i].value;
                y.options.add(item);
                x.remove(i);
                i--;
            }
        }
        y.selectedIndex = y.options.length - 1;
        y.focus();
    }

    function _saveRecord(m_lookup, m_select, saveType) {
        var strSelect = document.getElementById(m_select).value;
        var strLookup = document.getElementById(m_lookup).value;
        var value1 = getLookupSelectValues(strSelect);
        var value2 = getLookupSelectValues(strLookup);
        var value = value1 + value2;
        _doPostBack(saveType, value);
    }

    function getLookupSelectValues(str) {
        var sourceIDs = new Array();
        if (str.length > 0)
            if (str.substring(";") == -1)
                sourceIDs[0] = str;
            else sourceIDs = str.split(";");
        var value = "";
        for (i = 0; i < sourceIDs.length; i++) {
            var listBox = document.getElementById(sourceIDs[i]);
            value = value + sourceIDs[i];
            for (j = 0; j < listBox.length; j++) {
                value = value + ";#" + listBox.options[j].value;
            }
            value = value + sourceIDs[i];
        }
        return value;
    }

