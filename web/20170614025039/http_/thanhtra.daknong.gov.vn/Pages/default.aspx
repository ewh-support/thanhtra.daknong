    function loadCalendar() {
        var itemChosen = document.getElementById('ctl00_m_g_a085642a_5d29_422d_a261_e499f89e842f_ctl00_cbCondition');
        var calendar = document.getElementById('ctl00_m_g_a085642a_5d29_422d_a261_e499f89e842f_ctl00_searchByDate');
        var searchcontent = document.getElementById('ctl00_m_g_a085642a_5d29_422d_a261_e499f89e842f_ctl00_txtCongviec');
        var itemValue = itemChosen.options[itemChosen.selectedIndex].value;
        var typeIndex = itemValue.lastIndexOf(".");
        var type = itemValue.substr(typeIndex + 1, itemValue.length - typeIndex - 1);
        if (type == "DateTime") {
            calendar.style.display = "";
            searchcontent.disabled = false;
            searchcontent.style.backgroundColor = "white";
        }
        else {
            calendar.style.display = "none";
            searchcontent.disabled = false;
            searchcontent.style.backgroundColor = "white";
        }
    }

    function _getItem(eventTarget, m_object) {
        var value = document.getElementById(m_object).value;
